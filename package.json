{
  "name": "search-engine",
  "version": "1.0.0",
  "description": "CMSC 191 search engine client",
  "main": "index.js",
  "author": "John Leonard Tubig <jlu.tubig@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:lint": "standard 'src/**/*.js' --verbose --parser babel-eslint | snazzy",
    "test:unit": "babel-tape-runner src/**/*.spec.js | faucet",
    "test": "run-s test:lint test:unit",
    "prebuild:doc": "shx rm -rf docs",
    "prebuild:dst": "shx rm -rf dist; shx rm -f stats-dst.html",
    "build:doc": "esdoc -c esdoc.json",
    "build:dst": "webpack --progress --config webpack.config.prod.js",
    "build": "run-p build:dst",
    "postbuild": "opn stats-dst.html",
    "dev": "webpack-dev-server --config webpack.config.dev.js --open",
    "serve": "static dist --gzip --spa"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "debug": "2.6.3",
    "material-ui": "0.17.2",
    "normalize.css": "5.0.0",
    "rc-pagination": "1.8.5",
    "rc-select": "6.8.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-tap-event-plugin": "2.0.1",
    "regenerator-runtime": "0.10.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-tape-runner": "2.0.1",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.28.0",
    "esdoc": "0.5.2",
    "extract-text-webpack-plugin": "2.1.0",
    "faucet": "0.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "node-static": "0.7.9",
    "npm-run-all": "4.0.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "react-hot-loader": "next",
    "shx": "0.2.2",
    "snazzy": "6.0.0",
    "standard": "10.0.0",
    "style-loader": "0.16.1",
    "tape": "4.6.3",
    "tape-async": "2.1.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "private": true
}
