{
  "name": "search-engine",
  "version": "1.0.0",
  "description": "CMSC 191 search engine client",
  "main": "index.js",
  "author": "John Leonard Tubig <jlu.tubig@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaimaevi/InformationRetrieval.git"
  },
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  },
  "scripts": {
    "__1": "===================================  DOCUMENTATION  ===================================",
    "prebuild:doc:src": "shx rm -rf docs/src",
    "build:doc:src": "esdoc -c esdoc.json",
    "prebuild:doc:api": "shx rm -rf docs/api",
    "build:doc:api": "apidoc -i src -o docs/api -f \".*\\.?routes.js$\" -f \".*\\.?apidoc.js$\"",
    "__2": "=======================================  CLIENT  ======================================",
    "client:test:lint": "standard 'src/client/**/*.js' --verbose --parser babel-eslint | snazzy",
    "client:test:unit": "babel-tape-runner src/client/**/*.spec.js | faucet",
    "prebuild:client": "shx rm -rf dist/client; shx rm -f dist/stats-dst.html",
    "build:client": "webpack --progress --config webpack.config.prod.js",
    "postbuild:client": "opn dist/stats-dst.html",
    "dev:client": "webpack-dev-server --config webpack.config.dev.js --open",
    "__3": "=======================================  SERVER  ======================================",
    "server:test:lint": "standard 'src/server/**/*.js' --verbose --parser babel-eslint | snazzy",
    "server:test:unit": "babel-tape-runner src/server/**/*.spec.js | faucet",
    "prebuild:server": "shx rm -rf dist/server",
    "build:server": "babel src/server -d dist/server -s --copy-files",
    "dev:server": "nodemon --watch 'src/server/**/*.js' --exec npm run start:dev",
    "__4": "=======================================  START  =======================================",
    "start:dev": "node --require dotenv/config --require babel-register src/server/index.js",
    "start:prod": "node --require dotenv/config dist/server/index.js",
    "start": "node dist/server/index.js",
    "__5": "========================================  MISC  =======================================",
    "test:unit": "run-s client:test:lint server:test:lint",
    "test:lint": "run-s client:test:unit server:test:unit",
    "test": "run-s test:lint test:unit",
    "build:doc": "run-p build:doc:*",
    "build": "run-s build:server build:client"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "cors": "2.8.3",
    "debug": "2.6.3",
    "express": "4.15.2",
    "express-static-gzip": "0.2.2",
    "figlet": "1.2.0",
    "lodash": "4.17.4",
    "material-ui": "0.17.2",
    "mysql": "2.13.0",
    "normalize.css": "5.0.0",
    "polyfill": "0.1.0",
    "rc-pagination": "1.8.5",
    "rc-select": "6.8.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-tap-event-plugin": "2.0.1",
    "regenerator-runtime": "0.10.3",
    "superagent": "3.5.2",
    "superagent-bluebird-promise": "4.2.0"
  },
  "devDependencies": {
    "apidoc": "0.17.5",
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-tape-runner": "2.0.1",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.28.0",
    "dotenv": "4.0.0",
    "esdoc": "0.5.2",
    "extract-text-webpack-plugin": "2.1.0",
    "faucet": "0.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "react-hot-loader": "next",
    "shx": "0.2.2",
    "snazzy": "6.0.0",
    "standard": "10.0.0",
    "style-loader": "0.16.1",
    "tape": "4.6.3",
    "tape-async": "2.1.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "private": true
}
